<div id="content">
  <div id="mainbar">
    <div class="subheader index">
      <% if(typeOfIndex==="All") {%>
        <h1>All Questions</h1>
      <% } %>
      <% if(typeOfIndex==="User upvoted") {%>
        <h1>Upvoted Questions</h1>
      <% } %>      
      <% if(typeOfIndex==="User authored") {%>
        <h1>Authored Questions</h1>
      <% } %>      
