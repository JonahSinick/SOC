<h1>Sign In</h1>

<form action= "<%= session_url %>" method="post">
  
  <input 
    type="hidden"
    name="authenticity_token"
    value="<%= form_authenticity_token%>"
  >
  
  
  <label>Email
  <input 
    type="text"
    name="user[email]"
    value=""
  >
  </label>


  <label>Password
  <input 
    type="text"
    name="user[password]"
  >
  </label>
  
  <button type="submit">Sign In</button>

  
</form>